<!--
@file playbag.launch Plays back the bag file.
    Sets up image decompressors if needed, reads the urdf robot description.

@arg bag_file  Absolute path to the playing bag.
    default: $(env ROS_BAGS)/atlascar2/calibration_5_dez_2019_03m-07m.bag
@arg bag_start Playback starting time (in seconds). default: 0.0
@arg bag_rate  Playback rate. default: 1.0
-->

<launch>

<arg name="bag_file"  default="$(env ROS_BAGS)/atlascar2/calibration_5_dez_2019_03m-07m.bag"/>
<arg name="bag_start" default="0"/>
<arg name="bag_rate"  default ="1" />
<arg name="rviz_file" default="$(find atlascar2_calibration)/rviz/set_initial_estimate.rviz"/>
<arg name="description_file" default="$(find atlascar2_calibration)/urdf/initial_estimate.urdf.xacro"/>

<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
<!-- Load robot description and tf generators -->
<param name="robot_description"     command="$(find xacro)/xacro $(arg description_file)"   />
<node  name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->

<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
<!-- Playback the bag file -->
<param name="/use_sim_time" value="true"/>
<node pkg="rosbag" type="play" name="rosbag_play" output="screen"
    args=" $(arg bag_file) --clock -r $(arg bag_rate) -l -s $(arg bag_start) -d 2 /tf:=/tf_dev_null /tf_static:=/tf_static_dev_null"/>
<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->


<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
<!-- Image Topic Decompression -->
<node pkg="image_transport" type="republish" name="republish_top_left_camera" output="screen" args="compressed in:=/top_left_camera/image_color raw out:=/top_left_camera/image_color"/>
<node pkg="image_transport" type="republish" name="republish_top_right_camera" output="screen" args="compressed in:=/top_right_camera/image_color raw out:=/top_right_camera/image_color"/>

<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->


<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
<!-- Visualization -->
<node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rviz_file)" required="true"/>
<!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->

</launch>